CREATE DATABASE Cities;
USE Cities;

CREATE TABLE listOfCities (
  id INT PRIMARY KEY AUTO_INCREMENT,
  name VARCHAR(100) NOT NULL,
  logo VARCHAR(500),
  image VARCHAR(500),
  description VARCHAR(1000),
  population INT,
  average_salary INT,
  air_temperature_winter INT,
  air_temperature_summer INT,
  ap_price_meter INT,
  fuel_cost INT,
  unemployment_rate INT,
  top_position INT,
  bus_ticket INT
);

INSERT INTO listOfCities
	(name, logo, image, description, population, average_salary, air_temperature_winter, air_temperature_summer, ap_price_meter, fuel_cost, unemployment_rate, top_position, bus_ticket)
